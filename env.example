# Guerrilla Music Store - Environment Variables Template
# Copy this file to .env and fill in your actual values

# PayMe.io Configuration
REACT_APP_PAYME_API_KEY=your_payme_api_key_here
REACT_APP_PAYME_WEBHOOK_URL=https://your-domain.com/api/payme/webhook
REACT_APP_PAYME_ENVIRONMENT=sandbox

# Authentication
REACT_APP_JWT_SECRET=your_jwt_secret_here
REACT_APP_API_BASE_URL=https://your-api-domain.com/api

# Security
REACT_APP_ENCRYPTION_KEY=your_32_character_encryption_key_here

# reCAPTCHA
REACT_APP_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here

# Analytics (optional)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Development
REACT_APP_DEBUG_MODE=false
GENERATE_SOURCEMAP=false
